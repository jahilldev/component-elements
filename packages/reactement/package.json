{
  "name": "reactement",
  "version": "1.1.0",
  "author": "James Hill <contact@jameshill.dev>",
  "homepage": "https://github.com/jahilldev/component-elements/tree/main/packages/reactement#readme",
  "license": "MIT",
  "main": "./dist/define.es5.js",
  "types": "./dist/define.d.ts",
  "exports": {
    ".": "./dist/define.js",
    "./es5": {
      "type": "./dist/define.d.ts",
      "import": "./dist/define.es5.js",
      "require": "./dist/define.es5.js"
    }
  },
  "typesVersions": {
    "*": {
      "es5": [
        "./dist/define.d.ts"
      ]
    }
  },
  "keywords": [
    "react",
    "custom elements",
    "web components",
    "virtual dom",
    "partial hydration",
    "universal",
    "isomorphic",
    "hydrate",
    "component"
  ],
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jahilldev/component-elements.git"
  },
  "scripts": {
    "start": "run-s clean watch",
    "build": "webpack --mode=production",
    "watch": "webpack --watch",
    "clean": "rimraf ./dist",
    "lint": "eslint",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/jahilldev/component-elements/issues"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@component-elements/shared": "1.0.0",
    "@types/enzyme": "^3.10.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
